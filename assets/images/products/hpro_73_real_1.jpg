<!-- SCRIPT PER POPUP -->
<script>
document.addEventListener("DOMContentLoaded", function () {
  const storageKey = "popupClosedUntil";
  const now = new Date();

  // Controlla se il popup è già stato chiuso per 5 giorni
  const closedUntil = localStorage.getItem(storageKey);
  if (closedUntil && new Date(closedUntil) > now) {
    return;
  }

  // Attendi 3 secondi prima di cercare il popup (così diamo tempo anche al DOM di caricarlo completamente)
  setTimeout(() => {
    const popup = document.querySelector(".popup-container");
    if (!popup) return; // esce se il popup non esiste in questa pagina

    popup.style.display = "flex";

    const closeBtn = popup.querySelector(".press-close");
    closeBtn?.addEventListener("click", () => {
      popup.style.display = "none";

      const hideUntil = new Date();
      hideUntil.setDate(hideUntil.getDate() + 0);
      localStorage.setItem(storageKey, hideUntil.toISOString());
    });
  }, 3000);
});
